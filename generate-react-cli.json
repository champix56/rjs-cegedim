{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "ts-ui-nolazy":{
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "customTemplates":{
        "component":"grc-templates/ts-ui/TemplateName.tsx"
      }
    },
    "js-ui-lazy":{
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc-templates/js-ui/TemplateName.jsx"
      }
    }
  }
}